    "class-validator": "^0.14.1",
    "cros": "^1.0.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.3",
    "event-emitter": "^0.3.5",
    "form-data": "^4.0.0",
    "fs": "^0.0.1-security",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.6.0",
    "passport-42": "^1.2.6",
    "passport-jwt": "^4.0.1",
